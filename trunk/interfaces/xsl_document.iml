<?xml version="1.0" encoding="utf-8"?>
<class xmlns="http://www.sylma.org/interface">
  <file>XML.php</file>
  <name>XSL_Document</name>
  <extends>XML_Document</extends>
  <method path="get-parameter" name="getParameter">
    <argument required="true" format="php-string" name="local-name"/>
  </method>
  <method path="remove-parameter" name="removeParameter">
    <argument required="true" format="php-string" name="local-name"/>
  </method>
  <method path="set-parameter" name="setParameter">
    <argument required="true" format="php-string" name="name"/>
    <argument required="true" name="value">
      <format>php-string</format>
      <format>php-integer</format>
    </argument>
  </method>
  <method path="include-externals" name="includeExternals"/>
</class>
