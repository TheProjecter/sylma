<?xml version="1.0" encoding="utf-8"?>
<class xmlns="http://www.sylma.org/interface">
  <name>HTML_Form</name>
  <extends>XML_Document</extends>
  <method path="set-values" name="setValues">
    <multiple-arguments>
      <format>XML_Document</format>
    </multiple-arguments>
  </method>
  <method path="set-schemas" name="setSchemas">
    <multiple-arguments>
      <format>XML_Document</format>
    </multiple-arguments>
  </method>
  <method path="set-content" name="setContent">
    <multiple-arguments>
      <format>XML_Element</format>
      <format>XML_Document</format>
    </multiple-arguments>
  </method>
  <method path="add-cancel" name="addCancel"/>
  <method path="add-action" name="addAction">
    <multiple-arguments>
      <format>php-string</format>
      <format>XML_Element</format>
    </multiple-arguments>
  </method>
</class>
