<?xml version="1.0" encoding="utf-8"?>
<le:action xmlns:le="http://www.sylma.org/execution" xmlns:li="http://www.sylma.org/interface" xmlns="http://www.w3.org/1999/xhtml">
  <le:settings>
    <le:name>Dispatch</le:name>
    <le:argument name="list" format="SimpleNode"/>
  </le:settings>
  <le:switch>
    <le:get-argument/>
    <le:case>
      <le:argument>view</le:argument>
    </le:case>
    <le:case test="add">
      <le:action path="form">
        <le:get-argument le:name="action" name="list"/>
        <le:argument le:name="source">add_do</le:argument>
      </le:action>
    </le:case>
    <le:case test="add_do">
      <le:get-argument name="list">
        <li:add/>
      </le:get-argument>
    </le:case>
    <le:default>
      <div>Index</div>
    </le:default>
  </le:switch>
</le:action>
