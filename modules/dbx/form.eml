<?xml version="1.0" encoding="utf-8"?>
<le:action xmlns:le="http://www.sylma.org/execution" xmlns:li="http://www.sylma.org/interface" xmlns="http://www.w3.org/1999/xhtml" xmlns:lf="/sylma/processors/form/schema">
  <le:settings>
    <le:name>Formulaire</le:name>
    <le:argument format="XML_Document" name="model"/>
    <le:argument format="php-string" name="action"/>
  </le:settings>
  <le:action path="/modules/calendar/eightysix"/>
  <le:window>
    <li:add-css le:format="string">
      <le:directory/>
      <le:string>/form.css</le:string>
    </li:add-css>
    <li:add-js le:format="string">
      <le:directory/>
      <le:string>/form.js</le:string>
    </li:add-js>
  </le:window>
  <le:get-argument name="model">
    <li:parse>
      <le:template path="form.xsl">
        <li:set-parameter>
          <le:argument>action</le:argument>
          <le:get-argument name="action"/>
        </li:set-parameter>
      </le:template>
    </li:parse>
  </le:get-argument>
</le:action>
