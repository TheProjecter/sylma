<?xml version="1.0" encoding="utf-8"?>
<le:action xmlns:le="http://www.sylma.org/parser/action" xmlns="http://www.w3.org/1999/xhtml" xmlns:lm="http://www.sylma.org/parser/action/basic" xmlns:li="http://www.sylma.org/parser/caller" xmlns:test="http://www.sylma.org/modules/tester">
  <le:settings>
    <le:document le:set-variable="doc">
      <test:tests>
        <le:interface path="/#sylma/core/argument/test/interface"/>
        <le:interface path="/#sylma/core/factory/test/interface"/>
        <le:action path="/#sylma/storage/fs/test/group"/>
        <le:action path="/#sylma/parser/action/test/group"/>
        <le:interface path="/#sylma/parser/security/test/interface"/>
        <le:interface path="/#sylma/core/module/test/interface"/>
        <le:interface path="/#sylma/dom/test/interface"/>
        <le:interface path="/#sylma/dom/argument/test/interface"/>
      </test:tests>
    </le:document>
  </le:settings>
  <le:context name="title">Tests</le:context>
  <le:context name="css">
    <lm:file path="index.css"/>
  </le:context>
  <div class="sylma-tests">
    <le:action path="/#sylma/parser/js/binder/test"/>
    <lm:template path="index.xsl">
      <li:call name="parse">
        <le:get-variable name="doc"/>
      </li:call>
    </lm:template>
  </div>
</le:action>
