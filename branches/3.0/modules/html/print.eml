<?xml version="1.0" encoding="utf-8"?>
<le:action xmlns:le="http://www.sylma.org/execution" xmlns:li="http://www.sylma.org/interface" xmlns="http://www.w3.org/1999/xhtml" xmlns:ls="http://www.sylma.org/security">
  <le:settings>
    <le:name>Template HTML Print</le:name>
    <le:get-settings run="true" set-variable="language">infos/language</le:get-settings>
    <le:get-settings run="true" set-variable="icone">infos/icone</le:get-settings>
    <le:get-settings run="true" set-variable="webmaster-href">infos/webmaster/@href</le:get-settings>
  </le:settings>
  <html>
    <head>
      <title>
        <le:get-settings>infos/name</le:get-settings>
      </title>
      <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
      <meta http-equiv="content-language" content="[$language]"/>
      <link rel="icon" href="[$icone]" type="image/x-icon"/>
      <link rel="stylesheet" type="text/css" media="all" href="/sylma/css/global.css"/>
      <link rel="stylesheet" type="text/css" media="all" href="/sylma/css/messages.css"/>
    </head>
    <body style="padding: 1em; font-family: Tahoma; font-size: 12px;">
      <div id="content">
        <le:get-argument name="window-action"/>
      </div>
    </body>
  </html>
</le:action>
