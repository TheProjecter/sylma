<arg:argument xmlns:arg="http://www.sylma.org/core/argument" xmlns="http://www.sylma.org/parser/action/manager" xmlns:cls="http://www.sylma.org/core/factory">
  <!-- This document is not directly used by manager (manager.yml is the one) but imported from externals -->
  <debug>
    <show arg:type="boolean">1</show>
    <run arg:type="boolean">1</run>
    <update arg:type="boolean">0</update>
  </debug>
  <cls:classes>
    <cls:base>\sylma\core\argument</cls:base>
    <cls:class alias="handler" name="parser\Handler"/>
    <cls:class alias="compiler" name="parser\compiler\Main"/>
    <cls:class alias="window" name="\sylma\parser\languages\php\basic\Window"/>
    <cls:class alias="cached" name="parser\Cached"/>
    <php>
      <arg:import>/#sylma/parser/languages/php/window.xml</arg:import>
    </php>
  </cls:classes>
</arg:argument>
